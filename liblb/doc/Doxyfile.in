PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         = @VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@
STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@/lib@LIBNAME@/src \
			                   @PROJECT_SOURCE_DIR@/include/@LIBNAME@
INPUT                  = @PROJECT_SOURCE_DIR@/lib@LIBNAME@/src \
			                   @PROJECT_SOURCE_DIR@/include/@LIBNAME@
FILE_PATTERNS          = *.h \
                         *.cpp
RECURSIVE              = YES
GENERATE_LATEX	       = NO
HTML_OUTPUT            = @CMAKE_LIBRARY_OUTPUT_DIRECTORY@/doc
PREDEFINED             = DOXYGEN_SHOULD_SKIP_THIS
