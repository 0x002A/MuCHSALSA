PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         = @VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@
STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@/src \
			 @PROJECT_SOURCE_DIR@/../include/@TARGET@ \
                         @PROJECT_BINARY_DIR@
INPUT                  = @PROJECT_SOURCE_DIR@ \
			 @PROJECT_SOURCE_DIR@/../include/@TARGET@ \
                         @PROJECT_BINARY_DIR@
FILE_PATTERNS          = *.h \
                         *.cpp
RECURSIVE              = YES
GENERATE_LATEX	       = NO
HTML_OUTPUT            = @CMAKE_LIBRARY_OUTPUT_DIRECTORY@/doc
